{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
        ".ts"
        ]
      }
    }
  },
  "rules": {
    "import/extensions": [
      "error",
      "never",
      {
        "ignorePackages": true,
        "ts": "never"
      }
    ]
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "browser": true,
    "node": true
  }
}
  